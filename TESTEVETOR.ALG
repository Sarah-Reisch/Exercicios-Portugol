Algoritmo "TesteVetor"


Var

   v: vetor [1..6] de Inteiro
   c: Inteiro

Inicio

      Para c <- 1 ate 6 faca
           Escreva("Digite o",c," o valor: ")
           Leia(v[c]) //Leia o valor "v" na posição "c"
      FimPara
      
      Para c <- 1 ate 6 faca
           Escreva("[",v[c],"]")
      FimPara

Fimalgoritmo