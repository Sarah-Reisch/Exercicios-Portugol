Algoritmo "ImparePar"

Var

   C,Nu:Inteiro

Inicio

      Escreva("Digite um Número: ")
      Leia(Nu)
      EscrevaL("Números Pares: ")
      Para C <- 0 ate Nu passo 2 faca
           Escreval(C)
      FimPara
      EscrevaL("Números Ímpares: ")
      Para C <- 1 até Nu passo 2 faca
           Escreval(C)
      FimPara

Fimalgoritmo